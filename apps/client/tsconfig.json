{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "jsx": "react",
    "lib": [
			"DOM",
			"ESNext"
		],
		"types": [
      "vite/client",
			"node",
			"jest"
    ],
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
    "strict": true,
    "noEmit": true,
		"moduleResolution": "node",
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true
  }
}
